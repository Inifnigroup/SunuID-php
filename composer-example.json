{
    "name": "your-project/example",
    "description": "Exemple d'utilisation du SDK SunuID PHP",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "sunuid/php-sdk": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sunuid/php-sdk"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 